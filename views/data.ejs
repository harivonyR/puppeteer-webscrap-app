<%- include('partials/header') -%>
<div class="container mt-5">
    <div class="d-flex justify-content-end w-100 mb-5">
        <div>
            <a href="/data" class="btn btn-primary">Scrap Data</a>
            <a href="/download" class="btn btn-primary">Download Data</a>
        </div>
    </div>
    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>Batch</th>
                <th>Document</th>
                <th>Status</th>
            </tr>
        </thead>
            <tbody>
                <% rows.forEach((row)=> { %>
                    <tr>
                        <td><%= row.batch %></td>
                        <td><%= row.document%></td>
                        <td><%= row.status %></td>
                    </tr>
               <% }) %>
            </tbody>
        </table>
    </div>
<%- include('partials/footer') -%>